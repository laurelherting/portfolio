{
  "name": "portfolio",
  "version": "0.1.0",
  "description": "Laurel Herting portfolio site",
  "homepage": "https://laurelherting.github.io/portfolio",
  "author": {
    "name": "Laurel Herting",
    "url": "https://laurel.github.io/portfolio"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/laurelherting/laurelherting.github.io/portfolio"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.3.0",
    "yarn": ">= 1.0.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "watch": "webpack --watch",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.3.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.24.1",
    "carousel": "^0.0.3",
    "css-loader": "^0.28.7",
    "dedupe": "^2.1.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "file-loader": "^0.11.2",
    "font-awesome": "^4.7.0",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "moment": "^2.18.1",
    "occurrence": "^0.0.6",
    "order": "^0.1.0",
    "prettier": "^1.8.2",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^15.6.1",
    "react-global-configuration": "^1.2.0",
    "react-hot-loader": "^1.3.1",
    "react-image-resizer": "^1.2.0",
    "react-modal": "^2.3.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.12",
    "react-slick2": "^0.6.6",
    "react-waypoint": "^7.3.1",
    "style-loader": "^0.18.2",
    "superagent": "^3.6.0",
    "uglify-js": "^3.1.4",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.8.1",
    "webpack-merge": "^4.1.0",
    "webpack-s3-plugin": "^1.0.0-rc.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier",
      "prettier/react"
    ],
    "env": {
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 6
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "react/no-string-refs": "off",
      "react/no-unused-prop-types": "off",
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "import/no-extraneous-dependencies": "off",
      "no-console": "off",
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "parser": "flow",
          "trailingComma": "all"
        }
      ],
      "react/jsx-wrap-multilines": 0,
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    },
    "globals": {
      "fetch": true
    }
  }
}
